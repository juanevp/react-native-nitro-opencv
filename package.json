{
  "name": "react-native-nitro-opencv",
  "version": "0.0.1",
  "description": "react-native-nitro-opencv",
  "main": "lib/index",
  "module": "lib/index",
  "types": "lib/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "react-native.config.js",
    "lib",
    "nitrogen",
    "android/build.gradle",
    "android/gradle.properties",
    "android/CMakeLists.txt",
    "android/src",
    "ios/**/*.h",
    "ios/**/*.m",
    "ios/**/*.mm",
    "ios/**/*.cpp",
    "ios/**/*.swift",
    "app.plugin.js",
    "*.podspec",
    "README.md"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "tsc || exit 0;",
    "example": "pnpm --filter react-native-nitro-opencv-example",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf android/build node_modules/**/android/build lib",
    "lint": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "lint-ci": "eslint \"**/*.{js,ts,tsx}\" -f @jamesacarr/github-actions",
    "typescript": "tsc",
    "specs": "pnpm typescript -b ./tsconfig.specs.json && nitro-codegen --logLevel=\"debug\""
  },
  "keywords": [
    "react-native",
    "nitro"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanevp/react-native-nitro-opencv.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrousavy/react-native-nitro-opencv/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.38.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/bun": "^1.2.8",
    "@types/jest": "^29.5.12",
    "@types/react": "^19.0.6",
    "bun": "^1.2.7",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "nitro-codegen": "*",
    "prettier": "^3.5.3",
    "react": "19.0.0",
    "react-native": "0.78.1",
    "react-native-nitro-modules": "*",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-nitro-modules": "*"
  },
  "packageManager": "pnpm@10.6.5",
  "pnpm": {
    "onlyBuiltDependencies": [
      "bun"
    ]
  }
}
